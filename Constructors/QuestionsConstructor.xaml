<Window x:Class="PsychTestsMilitary.Constructors.QuestionsConstructor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PsychTestsMilitary.Constructors"
        mc:Ignorable="d"
        Title="Technique" Height="450" Width="800"
        Loaded="Window_Loaded">
    <Grid>
        <TextBox Name="number" BorderThickness="1" HorizontalAlignment="Left" Margin="70,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116" Height="29" IsReadOnly="True"/>
        <TextBox Name="description" BorderThickness="1" HorizontalAlignment="Left" Height="188" Margin="236,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="494"/>
        <Button Name="add" Content="Add Question" HorizontalAlignment="Left" Margin="236,295,0,0" VerticalAlignment="Top" Click="ButtonClicked" Height="61" Width="200"/>
        <ComboBox Name="techniques" HorizontalAlignment="Center" Margin="0,43,0,0" VerticalAlignment="Top" Width="661" BorderThickness="1" SelectionChanged="SelectionChanged"/>
        <Button Name="back" Content="Back" HorizontalAlignment="Left" Margin="546,316,0,0" VerticalAlignment="Top" Width="100" Click="ButtonClicked"/>

    </Grid>
</Window>
